<!DOCTYPE html>
<html>
  <head>
    <title>Movie App</title>
    <%- include("./head.ejs") %>
  </head>
  <body>
    <%- include("./navbar.ejs") %>

    <div class="container">
      <div class="row">
        <div class="col-sm-12">
          <h1>Now Playing</h1>
        </div>
        <div class="col-sm-12">
          <% for(let i=0; i<parsedData.length; i++) {%>
            <div class="col-md-3 col-sm-4 poster">
              <a href="/movie/<%= parsedData[i].id %>"><img src="<%= "http://image.tmdb.org/t/p/w300" + parsedData[i].poster_path %>" /></a>
            </div>
         <% } %>

        </div>
      </div>
    </div>
  </body>
</html>
